<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration>
<configuration>
    <appender name = "FILE-01" class = "ch.qos.logback.core.FileAppender">
        <file>D:\TEMP\logs\acelog\acelog.log</file>
        <encoder>
            <pattern>%date %level [%thread] %logger{10} [%file:%line] - %msg%n</pattern>
        </encoder>
    </appender>
    
    <root level = "debug">
        <appender-ref ref = "FILE-01" />
    </root>
    
</configuration>