  <dls:condition-list>
      <cnl:condition cnl:priority="4" rgr:name="trn">
        <cnl:condition-item cnl:regExp="NT.*">
            <cnl:group>middleOffice</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="N?[DFLNPS][BEOPRW][AOPW][DNOPT].*">
            <cnl:group>backOffice</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="\d{4}/\d{4}/\d{5}[BCc]?">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="\d{4}/\d{5}/\d{4}[BCc]?">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="[a-zA-Z]{4}/\d{4}">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
    </cnl:condition>
    <cnl:condition cnl:priority="1" rgr:name="fin">
        <cnl:condition-item cnl:regExp="7\d\d">
            <cnl:group>mt7XX</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="10\d">
            <cnl:group>zps</cnl:group>
            <cnl:group>middleOffice</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="199">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="999">
            <cnl:group>zps</cnl:group>
            <cnl:group>backOffice</cnl:group>
            <cnl:group>middleOffice</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="2\d\d">
            <cnl:group>middleOffice</cnl:group>
            <cnl:group>backOffice</cnl:group>
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="9[0145]0">
            <cnl:group>middleOffice</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="3\d\d">
            <cnl:group>backOffice</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="[19]9[1256]">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp="5\d\d">
            <cnl:group>mt5XX</cnl:group>
            <cnl:group>dealing</cnl:group>
            <cnl:group>boSecurities</cnl:group>
        </cnl:condition-item>
    </cnl:condition>
    <cnl:condition cnl:priority="3" rgr:name="event-number">
        <cnl:condition-item cnl:regExp="8[01][23][56]">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
    </cnl:condition>
    <cnl:condition cnl:priority="4" rgr:name="queue">
        <cnl:condition-item cnl:regExp="[M][o][n][e][y][L][a][u][n][d][e][r][i][n][g][IO][nu][t]?">
            <cnl:group>compliance</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp=".+">
            <cnl:group>dit</cnl:group>
        </cnl:condition-item>
    </cnl:condition>
    <cnl:condition cnl:priority="5" rgr:name="event">
        <cnl:condition-item cnl:regExp="trap">
            <cnl:group>zps</cnl:group>
            <cnl:group>middleOffice</cnl:group>
            <cnl:group>backOffice</cnl:group>
            <cnl:group>mt7XX</cnl:group>
            <cnl:group>boSecurities</cnl:group>
            <cnl:group>dit</cnl:group>
            <cnl:group>pdm</cnl:group>
        </cnl:condition-item>
    </cnl:condition>
    <cnl:condition cnl:priority="6" rgr:name="snf-file">
        <cnl:condition-item cnl:regExp="ou=sepa,o=deutdeff,o=swift">
            <cnl:group>zps</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp=".+trgt.+">
            <cnl:group>dt</cnl:group>
        </cnl:condition-item>
        <cnl:condition-item cnl:regExp=".+">
            <cnl:group>dit</cnl:group>
        </cnl:condition-item>
    </cnl:condition>
